{
  "distributed-cache": {
    "mode": "SYNC",
    "encoding": {
      "key": {
        "media-type": "application/x-protostream"
      },
      "value": {
        "media-type": "application/x-protostream"
      }
    },
    "transaction": {
      "mode": "NON_XA"
    },
    "memory": {
      "storage": "HEAP"
    }
  }
}

